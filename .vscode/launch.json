{
  "version": "0.2.0",
  "configurations": [
      {
          "name": "Launch index.html",
          "type": "chrome",
          "request": "launch",
          "file": "${workspaceRoot}/index.html",
          "webRoot": "${workspaceRoot}/src",
          "sourceMapPathOverrides": {
            "webpack:///src/*": "${webRoot}/*"
          },
          "sourceMaps": true
      }
  ]
}